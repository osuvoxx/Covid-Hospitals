<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 
<div class="jumbotron jumbotron-fluid container">
  <div class="container">
    <h1 class="display-4">Log in</h1>
   
   <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder:"Email" ,class: "form-control" %>
  </div>
  <br>
  <div class="field">
    <%= f.password_field :password, autocomplete: "current-password",placeholder:"Password" ,class: "form-control" %>
  </div>
  <br>
  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
    <br>
    <br>
     <%= render "devise/shared/links" %>
  </div>
  </div>
 
</div>


<% end %>